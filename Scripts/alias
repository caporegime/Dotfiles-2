# ObliviousGmn 
# Alias list

# Mirrors -- In Root Only
 alias Rankup='rankmirrors -n 6 /etc/pacman.d/mirrorlist.pacnew > /etc/pacman.d/mirrorlist'
 alias Rankedit='vim /etc/pacman.d/mirrorlist.pacnew'

# Pacman
 alias Pacman='sudo pacman -S'
 alias Paclook='sudo pacman -Ss'
 alias Pacrem='sudo pacman -Rcns'
 alias Pacupd='sudo pacman -Syyu'
 alias Pacsync='sudo pacman -Syy'
 alias Pacinfo='sudo pacman -Qi'
 alias PacAUR='yaourt -Syu --aur'
 alias Yaourt='yaourt -S'

# Git
 alias Dots='cd $HOME/.dotfiles && clear'
 alias Gadd='git add *'
 alias Gmit='git commit -m'
 alias Gpush='git push -u origin master'
 alias Gpull='git pull'

# Applications
 alias Suthunar='sudo thunar'
 alias svim='sudo vim'
 alias Yoump3='youtube-dl --extract-audio --audio-format mp3'

# Configs
 alias ls='ls --color=auto'
 alias clear='clear && Gmnbox'

# Custom
 alias Fontfix='gbdfed'
 alias Wallpaper='$HOME/Gmnbox/Bin/gempaper set'
 alias Section='apropos'
 alias Headphones='amixer set Line\ Out toggle'

# iPod
 alias iFuse='sudo modprobe fuse'
 alias iFusedown='killall ifuse'
 alias iFuseup='ifuse --root $HOME/.iPod && thunar $HOME/.iPod'

# System
 alias SetGame='xrandr --output VGA1 --mode 640x480'
 alias Copy='xclip -selection clipboard'
 alias Youtube_Fix='sudo iptables -I FORWARD -d 206.111.0.0/16 -j REJECT && sudo iptables -I FORWARD -d 173.194.55.0/24 -j REJECT'

# Usb
 alias Usbup='sudo mount -o gid=users,uid=oblivious,fmask=113,dmask=002 /dev/sdb1 .usbstick'
 alias Usbdown='sudo umount .usbstick'

# Systemctl Shortcuts

# Plex
 alias Plex='sudo systemctl start plexmediaserver'
 alias Plexrst='sudo systemctl restart plexmediaserver'
 alias Plexstop='sudo systemctl stop plexmediaserver'

# -- Plex library refresh
 alias Plex-refresh-Movies='curl http://localhost:32400/library/sections/7/refresh'
 alias Plex-refresh-TV='curl http://localhost:32400/library/sections/5/refresh'
 alias Plex-refresh-P90X='curl http://localhost:32400/library/sections/6/refresh'

# Dropbox
 alias Dbstart='sudo systemctl start dropbox@oblivious'
 alias Dbrst='sudo systemctl restart dropbox@oblivious'
 alias Dbstop='sudo systemctl stop dropbox@oblivious'

# Transmission
 alias Tranrst='sudo systemctl restart transmission'
 alias Transtop='sudo systemctl stop transmission'
 alias Transtart='sudo systemctl start transmission'
# -- 
 alias Pirate='transmission-remote-cli'

# Toilet
alias 2023='toilet -f future'
alias gaym='toilet --gay -f mono9 -t'
alias gayf='toilet --gay -f future -t'
alias gayt='toilet --gay -f term -t'
alias gayp='toilet --gay -f pagga -t'
alias metm='toilet --metal -f mono9 -t'
alias metf='toilet --metal -f future -t'
alias mett='toilet --metal -f term -t'
alias metp='toilet --metal -f pagga -t'
alias 3d='figlet -f 3d'

# End
